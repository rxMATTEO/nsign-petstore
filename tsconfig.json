{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxt/typescript-build",
      "@types/node",
      "./.nuxt/types/imports.d.ts",
      "./.nuxt/types/nitro-imports.d.ts",
      "types"
    ],
    "typeRoots": ["./.nuxt/types/imports.d.ts", "./.nuxt/types/nitro-imports.d.ts", "types"]
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "./.nuxt/tsconfig.json"
}